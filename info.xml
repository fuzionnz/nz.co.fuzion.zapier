<?xml version="1.0"?>
<extension key="zapier" type="module">
  <file>zapier</file>
  <name>CiviCRM Zapier Integration</name>
  <description>Triggers zap for civicrm events (supports 3 triggers)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>jitendrapurohit</author>
    <email>jitendra@fuzion.co.nz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/fuzionnz/zapier</url>
    <url desc="Documentation">https://github.com/fuzionnz/zapier/blob/master/README.md</url>
    <url desc="Support">https://github.com/fuzionnz/zapier</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-06-15</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <requires>
    <ext>org.civicoop.civirules</ext>
  </requires>
  <civix>
    <namespace>CRM/Zapier</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
